<keymap version="1" name="MyPyCharmKeyMap" parent="$default">
  <action id="Back">
    <mouse-shortcut keystroke="button4" />
    <keyboard-shortcut first-keystroke="alt left" />
  </action>
  <action id="CommentByLineComment">
    <keyboard-shortcut first-keystroke="ctrl slash" />
    <keyboard-shortcut first-keystroke="ctrl divide" />
    <keyboard-shortcut first-keystroke="shift ctrl 7" />
  </action>
  <action id="CompareTwoFiles" />
  <action id="Diff.ShowSettingsPopup" />
  <action id="EditorDuplicate">
    <keyboard-shortcut first-keystroke="shift ctrl d" />
  </action>
  <action id="EditorSelectLine">
    <keyboard-shortcut first-keystroke="ctrl l" />
  </action>
  <action id="EditorSelectWord">
    <keyboard-shortcut first-keystroke="ctrl w" />
    <keyboard-shortcut first-keystroke="shift ctrl space" />
    <keyboard-shortcut first-keystroke="ctrl d" />
  </action>
  <action id="EditorToggleCase">
    <keyboard-shortcut first-keystroke="shift ctrl u" />
    <keyboard-shortcut first-keystroke="ctrl k" second-keystroke="ctrl u" />
    <keyboard-shortcut first-keystroke="ctrl k" second-keystroke="ctrl l" />
  </action>
  <action id="FileChooser.GotoDesktop" />
  <action id="FindNext">
    <keyboard-shortcut first-keystroke="f3" />
  </action>
  <action id="Forward">
    <keyboard-shortcut first-keystroke="ctrl alt right" />
    <mouse-shortcut keystroke="button5" />
    <keyboard-shortcut first-keystroke="alt right" />
  </action>
  <action id="NextTab" />
  <action id="PreviousTab">
    <keyboard-shortcut first-keystroke="shift alt left" />
  </action>
  <action id="ToggleBookmark7" />
  <action id="Vcs.Log.FocusTextFilter" />
</keymap>